<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.github.han9912.zipin.invitation.mapper.InvitationMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO invitations (job_id, recruiter_id, seeker_id)
        VALUE (#{jobId}, #{recruiterId}, #{seekerId})
    </insert>
</mapper>